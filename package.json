{
  "name": "prettier-playground",
  "version": "0.1.0",
  "private": true,
  "description": "Playground to get formatting results using a specific version of Prettier.",
  "author": "Hyeonjong <nianelo4@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "rimraf .next && next dev",
    "build": "rimraf .next && rimraf src/pages/api && next build",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "test": "jest --ci --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@biomejs/biome": "1.7.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@trivago/prettier-plugin-sort-imports": "4.2.1",
    "@types/testing-library__jest-dom": "5.14.5",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "prettier": "3.0.3",
    "prettier-plugin-brace-style": "0.6.0",
    "prettier-plugin-classnames": "0.6.3",
    "prettier-plugin-merge": "0.6.0",
    "prettier-plugin-tailwindcss": "0.4.1",
    "rimraf": "5.0.1",
    "typescript": "4.9.5"
  },
  "packageManager": "pnpm@8.5.1"
}
